{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimiting": {
    "GlobalLimiter": {
      "PermitLimit": 100,
      "WindowSeconds": 60
    },
    "Policies": {
      "FivePerMinute": {
        "PermitLimit": 5,
        "WindowSeconds": 60
      },
      "TenPer30Seconds": {
        "PermitLimit": 10,
        "WindowSeconds": 30
      }
    }
  }
}
